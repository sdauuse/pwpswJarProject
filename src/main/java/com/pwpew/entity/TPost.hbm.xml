<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pwpew.entity.TPost" table="t_post" schema="welfare_people_seeking">
        <id name="postId">
            <column name="postId" sql-type="int(32)"/>
        </id>
        <property name="postType">
            <column name="postType" sql-type="varchar(16)" length="16"/>
        </property>
        <property name="postName">
            <column name="postName" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="postGender">
            <column name="postGender" sql-type="varchar(4)" length="4" not-null="true"/>
        </property>
        <property name="postAge">
            <column name="postAge" sql-type="int(4)" not-null="true"/>
        </property>
        <property name="missingtime">
            <column name="missingtime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="postProvince">
            <column name="postProvince" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="postCity">
            <column name="postCity" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="postCountry">
            <column name="postCountry" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="postStreet">
            <column name="postStreet" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="postDescribe">
            <column name="postDescribe" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="commentId">
            <column name="commentId" sql-type="int(16)"/>
        </property>
        <property name="effectiveness">
            <column name="effectiveness" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>